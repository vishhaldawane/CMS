class Farmer {
	void farming() {
		System.out.println("Farmer is farming...");
	}
	void sellCrop() {
		System.out.println("Farmer is selling crop...");
	}
}

class Insurance {
	void claim() {
		System.out.println("Claiming insurance....");
	}
}

class Shopping {
	void addItem() {
		System.out.println("Adding item to the cart...");
	}
	void study() {
		System.out.println("Student is studying....");
	}
	void whatsapping() {
		System.out.println("Student is whatsapping...");
	}
	void reeling() {
		System.out.println("student is reeling....");
	}
	
}

