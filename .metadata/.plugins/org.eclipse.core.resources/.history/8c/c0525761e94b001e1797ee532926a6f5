
public class ConstructorHierarchyTest {
	public static void main(String[] args) {
		
		GrandFather gf = new GrandFather();
		gf.farming();
	}
}

class GrandFather
{
	GrandFather() {
		System.out.println("GrandFather() ctor...");
	}
	void farming() {
		System.out.println("GrandFather is farming...");
	}
	
}
