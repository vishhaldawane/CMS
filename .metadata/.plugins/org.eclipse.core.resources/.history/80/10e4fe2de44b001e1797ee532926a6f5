
public class ChessTest {
	public static void main(String[] args) {
		Chess chess = new WebBasedGraphicalChess();
		chess.moveKnight();
		
	
		
		
		
		chess = new Chess();
		
		chess.moveKnight();
		
		
		chess = new GraphicalChess();
		
		chess.moveKnight();
	}
}

class Chess //Piyush
{
	void moveKnight() {//BOSS
		System.out.println("Moving Knight L-Shape....any direction...");
	}
}

class GraphicalChess extends Chess //Pranav
{
		//CHILD
	
	void moveKnight() {//BOSS
		System.out.println("Moving Knight L-Shape.. on graphics board....any direction...");
	}
	
}

class WebBasedGraphicalChess extends GraphicalChess //Kedar
{
	//GRAND CHILD
	void moveKnight() {//BOSS
		System.out.println("Moving Knight L-Shape...inside chrome...with graphics...any direction...");
	}
}
