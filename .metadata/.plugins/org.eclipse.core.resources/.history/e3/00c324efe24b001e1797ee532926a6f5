
public class ChessTest {
	public static void main(String[] args) {
		Chess chess = new GraphicalChess();
		chess.moveKnight();
		
		
	}
}

class Chess
{
	void moveKnight() {
		System.out.println("Moving Knight L-Shape....any direction...");
	}
}

class GraphicalChess extends Chess
{
	void moveKnight() {
		System.out.println("Moving Knight L-Shape....on graphics board....any direction..");
	}
}
